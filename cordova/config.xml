<?xml version='1.0' encoding='utf-8'?>
<widget id="nl.kabisa.kudometer" version="2.2.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Kudos-o-Matic</name>
    <description />
    <content src="index.html" />
    <access origin="*" />
    <hook src="../node_modules/maji/cordova/hooks/before_platform_add.js" type="before_platform_add" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="Orientation" value="portrait" />
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <preference name="SplashScreen" value="none" />
    <preference name="SplashScreenDelay" value="0" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="FadeSplashScreenDuration" value="0" />
    <preference name="android-minSdkVersion" value="21" />
    <preference name="android-targetSdkVersion" value="26" />
    <preference name="BackupWebStorage" value="local" />
    <allow-navigation href="*" />
    <icon src="../src/assets/logo/AppStore-1024.png" />
    <platform name="ios">
        <splash height="1136" platform="ios" src="../src/assets/screens/ios/Default-568h@2x~iphone.png" width="640" />
        <splash height="1334" platform="ios" src="../src/assets/screens/ios/Default-667h.png" width="750" />
        <splash height="2208" platform="ios" src="../src/assets/screens/ios/Default-736h.png" width="1242" />
        <splash height="1242" platform="ios" src="../src/assets/screens/ios/Default-Landscape-736h.png" width="2208" />
        <splash height="1536" platform="ios" src="../src/assets/screens/ios/Default-Landscape@2x~ipad.png" width="2048" />
        <splash height="2436" platform="ios" src="../src/assets/screens/ios/Default@3x~iphone~anycom.png" width="1242" />
        <splash height="768" platform="ios" src="../src/assets/screens/ios/Default-Landscape~ipad.png" width="1024" />
        <splash height="2048" platform="ios" src="../src/assets/screens/ios/Default-Portrait@2x~ipad.png" width="1536" />
        <splash height="1024" platform="ios" src="../src/assets/screens/ios/Default-Portrait~ipad.png" width="768" />
        <splash height="960" platform="ios" src="../src/assets/screens/ios/Default@2x~iphone.png" width="640" />
        <splash height="480" platform="ios" src="../src/assets/screens/ios/Default~iphone.png" width="320" />
        <hook src="scripts/target_attributes.rb" type="after_build" />
        <hook src="scripts/share_schemes.rb" type="after_platform_add" />
        <icon height="40" src="../src/assets/logo/AppStore-40.png" width="40" />
        <icon height="76" src="../src/assets/logo/AppStore-76.png" width="76" />
        <icon height="120" src="../src/assets/logo/AppStore-120.png" width="120" />
        <icon height="152" src="../src/assets/logo/AppStore-152.png" width="152" />
    </platform>
    <platform name="android">
        <splash qualifier="land-ldpi" src="../src/assets/screens/android/drawable-land-ldpi-screen.png" />
        <splash qualifier="land-mdpi" src="../src/assets/screens/android/drawable-land-mdpi-screen.png" />
        <splash qualifier="land-hdpi" src="../src/assets/screens/android/drawable-land-hdpi-screen.png" />
        <splash qualifier="land-xhdpi" src="../src/assets/screens/android/drawable-land-xhdpi-screen.png" />
        <splash qualifier="land-xxhdpi" src="../src/assets/screens/android/drawable-land-xxhdpi-screen.png" />
        <splash qualifier="land-xxxhdpi" src="../src/assets/screens/android/drawable-land-xxxhdpi-screen.png" />
        <splash qualifier="port-ldpi" src="../src/assets/screens/android/drawable-port-ldpi-screen.png" />
        <splash qualifier="port-mdpi" src="../src/assets/screens/android/drawable-port-mdpi-screen.png" />
        <splash qualifier="port-hdpi" src="../src/assets/screens/android/drawable-port-hdpi-screen.png" />
        <splash qualifier="port-xhdpi" src="../src/assets/screens/android/drawable-port-xhdpi-screen.png" />
        <splash qualifier="port-xxhdpi" src="../src/assets/screens/android/drawable-port-xxhdpi-screen.png" />
        <splash qualifier="port-xxxhdpi" src="../src/assets/screens/android/drawable-port-xxxhdpi-screen.png" />
        <custom-config-file parent="/*" target="AndroidManifest.xml">
            <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
            <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
            <uses-permission android:name="android.permission.CHANGE_CONFIGURATION" />
        </custom-config-file>
        <hook src="scripts/prepare_screengrab.rb" type="after_build" />
    </platform>
    <plugin name="cordova-plugin-networkactivityindicator" spec="^0.1.1" />
    <plugin name="cordova-plugin-splashscreen" source="git" spec="https://github.com/egonm12/cordova-plugin-splashscreen#f5e2d17da8f1bdd5fe6149acff2b2ffc3fba5ec1" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-inappbrowser" spec="^3.0.0" />
    <engine name="android" spec="^7.1.2" />
    <engine name="ios" spec="^4.5.5" />
</widget>
